{
	"name": "wticons",
	"version": "1.0.11",
	"description": "Bundle of website development and technology icons",
	"main": "dist/index.js",
	"type": "module",
	"repository": "git@github.com:OzzyCzech/wticons.git",
	"author": "Roman OÅ¾ana <roman@ozana.cz>",
	"license": "MIT",
	"types": "./dist/index.d.ts",
	"files": [
		"/dist"
	],
	"scripts": {
		"rebuild": "yarn clean && yarn build",
		"clean": "rimraf ./tsx ./dist",
		"build:tsx": "svgr --out-dir tsx --typescript --index-template index.template.cjs -- icons",
		"build:js": "tsc",
		"build": "yarn build:jsx && yarn build:js",
		"optimize": "svgo -i ./icons"
	},
	"devDependencies": {
		"@svgr/cli": "^6.5.1",
		"react": "^18.2.0",
		"rimraf": "^3.0.2",
		"svgo": "^3.0.1",
		"typescript": "^4.9.3"
	},
	"keywords": [
		"programming",
		"webdesign",
		"technology",
		"icons",
		"svg"
	],
	"dependencies": {
		"react-native-svg": "^13.6.0"
	}
}
